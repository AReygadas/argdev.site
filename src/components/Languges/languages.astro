        <Icon class="iconS" name="logos:vue" />
        <Icon class="iconS" name="logos:react" />
        <Icon class="iconS" name="logos:html-5" />
        <Icon class="iconS" name="logos:css-3" />
        <Icon class="iconS" name="logos:tailwindcss-icon" />
